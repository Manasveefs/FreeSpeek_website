{
  "version": "0.2.0",
  "configurations": [
    {
      "outputCapture": "std",
      "name": "Launch Program",
      "program": "${workspaceFolder}/socket-server.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/"],
      "type": "pwa-node"
    },
    {
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "name": "nodemon",
      "program": "${workspaceFolder}/src/services/socket-server.js",
      "request": "launch",
      "restart": true,
      "runtimeExecutable": "nodemon",
      "skipFiles": ["<node_internals>/"],
      "type": "pwa-node"
    }
  ]
}
